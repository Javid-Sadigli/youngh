<%- include('includes/head.ejs') %>

    <link rel="stylesheet" href="/css/signin.css">
</head>
<body>
  <%- include('includes/header.ejs') %>

    

<div class="container">
    <div class="main">
        <h1>Sign in</h1>
                <form action="/signin" method="post">
            <label for="first">
              Email : 
            </label>
			<div class="input-container">
            <input name="email" type="text" placeholder="Enter Your Email" required>
		</div>
            <label for="third">
                Password:
            </label>
			<div class="input-container">
 		
            <input name="password" type="password" placeholder=" Enter Password"  required id="password" name="password">
		</div>
         

          <div class="wrap">
            <button type="submit" onclick="solve()">
              <b>Submit</b> 

            </button>
          </div>
        </form>
        <p>Not registered? <a href="signup.html" style="text-decoration: none;color: white; transition: 2s;" ><b>Create an account</b></a></p>
      
        <span style="color: white;">--------or sign in  with--------</span>
        <div>
          <ul class="sign-in-up-with">
            <li class="with-in"><a href="#"><img src="/assets/icons/google-icon.png" width="30px" height="30px" alt=""></a></li>
            <li class="with-in"><a href="#"><img src="/assets/icons/facebook.png" width="30px" height="30px" alt=""></a></li>
          </ul>
        </div>


    </div>
    

</div>
</body>
</html>